{
  "name": "davidsneighbour/booka-cli",
  "description": "CLI workforce for BooKa",
  "version": "1.0.9",
  "type": "library",
  "authors": [
    {
      "name": "Patrick Kollitsch",
      "email": "patrick@davids-neighbour.com",
      "homepage": "https://davids-neighbour.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Booka\\Cli\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "autoload": "composer dump-autoload --optimize --classmap-authoritative --apcu",
    "phan": "PHAN_DISABLE_XDEBUG_WARN=1 && phan -k .phan/config.php --load-baseline .phan/baseline.php -C --color-scheme=vim --progress-bar -b -x -t -z -S",
    "phan-html": "phan -k .phan/config.php --load-baseline .phan/baseline.php -C --color-scheme=vim --progress-bar -b -x -t -z -S -m=html -o=phan.html",
    "phan-baseline": "phan -k .phan/config.php --load-baseline .phan/baseline.php -C --color-scheme=vim --progress-bar -b -x -t -z -S --save-baseline .phan/baseline.php",
    "phpstan": "phpstan analyse -c phpstan.neon src -l max --ansi",
    "phpstan-baseline": "phpstan analyse -l 8 -c phpstan.neon --generate-baseline -- src",
    "psalm": "psalm.phar --show-info=true",
    "psalm-baseline": "psalm.phar --set-baseline=psalm-baseline.xml"
  },
  "require": {
    "php": "^7.4",
    "consolidation/robo": "2.1.0",
    "league/flysystem": "1.0.69",
    "phpstan/extension-installer": "1.0.4",
    "phpstan/phpstan": "0.12.25",
    "phpstan/phpstan-phpunit": "0.12.11",
    "phpstan/phpstan-strict-rules": "0.12.2",
    "thecodingmachine/phpstan-strict-rules": "0.12.0",
    "thecodingmachine/safe": "1.1.2",
    "curl/curl": "2.3"
  },
  "require-dev": {
    "phan/phan": "3.0.0",
    "phpstan/phpstan-deprecation-rules": "0.12.4",
    "psalm/phar": "3.11.5",
    "codeception/codeception": "4.1"
  }
}
